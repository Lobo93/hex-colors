--[[pod_format="raw",created="2025-07-15 16:21:49",modified="2025-07-15 18:17:09",revision=217]]
function _init()
	vid(4)

	hex_codes={
		[0]="#000000","#1d2b53","#7e2553","#008751",
		"#ab5236","#5f574f","#c2c3c7","#fff1e8",
		"#ff004d","#ffa300","#ffec27","#00e436",
		"#29adff","#83769c","#ff77a8","#ffccaa",
		"#2463b0","#00a5a1","#654688","#125359",
		"#742f29","#452d32","#a28879","#ffacc5",
		"#b9003e","#e26b02","#95f042","#00b251",
		"#64dff6","#bd9adf","#e40dab","#ff8557"
	}

	gui=create_gui()

	for i=0,31 do
		gui:attach {
			x=i*10%160, y=flr(i/16)*10, width=10, height=10,
			draw=function(self)
				rectfill(1,1,9,9,i)
			end,
			click=function(self)
				cls()
				rectfill(1,21,79,88,i)
				?hex_codes[i],81,21,max(1,i)
				set_clipboard(hex_codes[i])
				notify("                Co\v7,digo copiado!")
			end
		}
	end
end

function _update()
	gui:update_all()
end

function _draw()
	gui:draw_all()
end
